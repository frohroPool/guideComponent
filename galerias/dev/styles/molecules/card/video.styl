.m-cardVideo
	width 250px
	height 250px
	display flex
	flex-flow row wrap
.m-cardVideo__media
	width 100%
	height 100%
.m-cardVideo__section
	text-transform uppercase
	font-size 10px
.m-cardVideo__title--isH3
	color black
.m-cardVideo__title--isH3:hover
	color gray
	cursor pointer
.m-cardVideo__time
	font-size 10px
	font-family 'Roboto Condensed',sans-serif
	color gray
	