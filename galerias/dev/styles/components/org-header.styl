.org-header
    display grid
    grid-template-columns repeat(12, 1fr)
    grid-template-rows 5vw 5vw  5vw 5vw    

.org-header__login-bar
    grid-column 1 / span 12    
    background #343851

.org-header__menu-bar
    grid-column 1 / span 12    
    background #111735

.list-simple-item     
    color #FFF

[data-mode="list-simple__list-pipe"] li  
 border-right:1px solid #FFF

    
.org-header__search-bar
    grid-column 1 / span 12    
    background orange